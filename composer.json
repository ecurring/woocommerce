{
    "name": "ecurring/woo-ecurring",
    "description": "Collect your subscription fees in WooCommerce with ease.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "dhii/output-renderer-interface": " ^0.4",
        "chrico/wp-fields": " ^1.0",
        "brain/nonces": " ^1.0",
        "ext-json": "*",
        "dhii/versions": "^0.1",
        "dhii/php-template": "^0.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5 || ^7.1 || ^9.3.0",
        "brain/monkey": "^2.4",
        "antecedent/patchwork": " ^2.1",
        "vimeo/psalm": "^4.4.0",
        "roots/wordpress": "@stable",
        "woocommerce/woocommerce": "^4",
        "mollie/mollie-woocommerce": "dev-develop",
        "inpsyde/php-coding-standards": " ^1",
        "woocommerce/action-scheduler": "^3.1.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mollie/WooCommerce"
        }
    ],
    "autoload": {
        "psr-0": {
            "eCurring_WC_": "./includes"
        },
        "psr-4": {
            "Ecurring\\WooEcurring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecurring\\WooEcurringTests\\": "tests"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:wordpress-plugin"]
        },
        "wordpress-install-dir": "vendor/wordpress/wordpress"
    }
}
