{
    "name": "ecurring/woo-ecurring",
    "description": "Collect your subscription fees in WooCommerce with ease.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^5.6.40 | ^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5 || ^7.1",
        "brain/monkey": "^2.4"
